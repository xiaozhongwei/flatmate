<div class="navigation">
    <div class="container ">
        <ul>
            <li><a href="#">HOME</a></li>
            <li><a href="#">PROFILE</a></li>
            <li><a href="#">LISTING</a></li>
            <li><a href="#">INBOX</a></li>
            <li><a href="#">WISHLIST</a></li>
            <li><a href="#">ABOUT</a></li>
        </ul>
    </div>
</div><!-- End Position -->
<div class="container margin_30">
    <div class="row">

        <aside class="col-lg-3 col-md-3">
            <div class="widget">
                <div id="tour_guide">
                    <p style="margin-bottom: 10px;">
                        <img src="/images/amaze_300x300.jpg" alt="" style="width:170px;" class="img-circle styled">
                    </p>
                    <h5 style="margin:5px 0;font-weight:bold">John Doe</h5>
                    <p>Member Since May 2012</p>
                </div>
            </div><!-- End Search -->
            <div class="box_style_cat">
                <ul id="cat_nav">
                    <li>{{#link-to 'profile.edit-profile' id="active"}}<i class="icon_set_1_icon-51"></i>Edit Profile{{/link-to}}</li>
                    <li>{{#link-to 'profile.photo'}}<i class="icon_set_1_icon-3"></i>Upload Photo{{/link-to}}</li>
                    <li>{{#link-to 'profile.change-password'}}<i class="icon_set_1_icon-4"></i>Security{{/link-to}}</li>
                    <li><a href="#"><i class="icon_set_1_icon-43"></i>Credit/Debit Cards</a></li>
                </ul>
            </div>
            <p>
                <a class="btn_map" href="/#/profile/view">View Profile</a>
            </p>
        </aside>

        <div class="col-md-9">

          {{#if isFull}}
            {{yield}}
          {{else}}
              <div class="panel panel-default">
                  <div class="panel-heading">
                      <h3 class="panel-title" style="font-size:22px;padding:5px 0;">{{_profileTitle}}</h3>
                  </div>
                  <div class="panel-body">
                    {{yield}}
                  </div>
              </div>
          {{/if}}
        </div><!-- End col-md-9-->
    </div><!-- End row-->
</div><!-- End container -->
