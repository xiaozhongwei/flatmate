<section class="parallax-window" style="min-height:270px;" data-parallax="scroll" data-image-src="/images/bg_blog.jpg" data-natural-width="1400" data-natural-height="270">
    <div class="parallax-content-1" style="height:270px;">
        <div class="animated fadeInDown">
            <h1>Tour Blog</h1>
            <p>Ridiculus sociosqu cursus neque cursus curae ante scelerisque vehicula.</p></div>
    </div>
</section><!-- End section -->

<div id="position">
    <div class="container">
        <ul>
            <li><a href="#">Home</a></li>
            <li><a href="#">Category</a></li>
            <li>Page active</li>
        </ul>
    </div>
</div><!-- End position -->

<div class="container margin_60">
    <div class="row">
        <aside class="col-md-3 add_bottom_30">

            <div class="widget">
                <div id="tour_guide">
                    <p>
                        <img src="/images/testimonial_1.jpg" alt="" class="img-circle styled">
                    </p>
                </div>
            </div><!-- End Search -->

            <hr>

            <div class="widget" id="cat_blog">
                <h4>Profile Management</h4>
                <ul>
                    <li>{{#link-to 'profile.dashboard'}}Dashboard{{/link-to}}</li>
                    <li>{{#link-to 'profile.edit-profile'}}Edit Profile{{/link-to}}</li>
                    <li>{{#link-to 'profile.photo'}}Upload Photo{{/link-to}}</li>
                    <li>{{#link-to 'profile.change-password'}}Security{{/link-to}}</li>
                    <li>{{#link-to 'profile.listing'}}Listing{{/link-to}}</li>
                    <li><a href="#">Credit/Debit Cards</a></li>
                    <li><a href="#">Wishlist</a></li>
                </ul>
            </div><!-- End widget -->

        </aside><!-- End aside -->

        <div class="col-md-9">

          {{#if isFull}}
            {{yield}}
          {{else}}
              <div class="panel panel-default">
                  <div class="panel-heading">
                      <h3 class="panel-title" style="font-size:22px;padding:5px 0;">{{_profileTitle}}</h3>
                  </div>
                  <div class="panel-body">
                    {{yield}}
                  </div>
              </div>
          {{/if}}
        </div><!-- End col-md-9-->
    </div><!-- End row-->
</div><!-- End container -->
